                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ubuntul"
                    android:textStyle="bold"
                    android:inputType="numberDecimal"
                    android:hint="Rs per km"
                    android:textSize="20dp"
                    android:visibility="visible"/>

            </RelativeLayout>

        </RelativeLayout>

    </ScrollView>

    <ScrollView
        android:layout_margin="5dp"
        android:id="@+id/menuscroll"
        android:layout_below="@+id/subtab"
        android:layout_above="@id/upload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/rellaymenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:stateListAnimator="@anim/animation"
                android:foreground="?attr/selectableItemBackground"
                android:id="@+id/select_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/yellow"
                android:fontFamily="@font/ubuntu"
                android:textAllCaps="false"
                android:text="Click to upload image (Optional)"
                android:textAlignment="center"
                android:background="@drawable/custom_button_black"
                android:layout_margin="10dp"/>

            <androidx.viewpager.widget.ViewPager
                android:id="@+id/display_image"
                android:layout_width="250dp"
                android:layout_height="250dp"
                android:layout_marginTop="5dp"
                android:layout_below="@+id/select_image"
                android:visibility="gone"
                android:layout_centerHorizontal="true"/>

            <com.google.android.material.tabs.TabLayout
                android:id="@+id/dots"
                android:layout_below="@+id/display_image"
                android:layout_width="match_parent"
                android:layout_height="14dp"
                app:tabBackground="@drawable/tab_selector"
                app:tabGravity="center"
                app:tabIndicatorHeight="0dp"
                android:visibility="visible"/>

            <TextView
                android:id="@+id/menu"
                android:textColor="@color/black"
                android:text="Click to add items in menu"
                android:fontFamily="@font/ubuntul"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/dots"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:textSize="20sp" />


            <com.example.bookmytiffin.speciallistview
                android:id="@+id/itempricelv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/menu"
                android:layout_margin="7dp">
            </com.example.bookmytiffin.speciallistview>

        </RelativeLayout>

    </ScrollView>

    <Button
