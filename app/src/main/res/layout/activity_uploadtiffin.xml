            <com.rengwuxian.materialedittext.MaterialEditText
                android:id="@+id/address"
                app:met_floatingLabel="normal"
                app:met_floatingLabelTextColor="@color/black"
                android:textStyle="bold"
                app:met_floatingLabelAnimating="true"
                app:met_floatingLabelTextSize="17dp"
                app:met_textColorHint="@color/black"
                android:textColor="@color/black"
                android:hint="Address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="12dp"
                android:textSize="20dp"
                app:met_underlineColor="@color/yellow"
                android:editable="false"
                android:cursorVisible="false"
                android:focusable="false"
                android:fontFamily="@font/ubuntul"
                android:layout_below="@+id/days"/>

            <ProgressBar
                android:id="@+id/addprogress"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_below="@+id/address"
