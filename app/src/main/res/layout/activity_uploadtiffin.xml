                <com.rengwuxian.materialedittext.MaterialEditText
                    android:id="@+id/range"
                    app:met_floatingLabel="normal"
                    app:met_floatingLabelTextColor="@color/black"
                    app:met_floatingLabelAnimating="true"
                    app:met_underlineColor="@color/yellow"
                    android:layout_marginLeft="10dp"
                    app:met_floatingLabelTextSize="17dp"
                    app:met_textColorHint="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ubuntul"
                    android:textStyle="bold"
                    android:inputType="numberDecimal"
                    android:hint="Delivery Range"
                    android:textSize="20dp"
                    android:visibility="visible"/>

                <!--<TextView
                    android:id="@+id/freedelititle"
                    android:layout_below="@+id/rangetitle"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ubuntul"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:text="Free Delivery : "
                    android:textColor="@color/black"
                    android:visibility="visible"/>-->

                <TextView
                    android:id="@+id/chargetitle"
                    android:layout_below="@+id/range"
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ubuntul"
                    android:layout_marginTop="15dp"
                    android:text="Charge per km for delivery: "
